{
  "name": "in2it-site",
  "version": "1.0.0",
  "description": "in2it site",
  "author": "WesThorburn",
  "private": true,
  "scripts": {
    "docker-debug": "NUXT_HOST=0.0.0.0 NUXT_PORT=3000 node -r ts-node/register --inspect=[::]:5859 /usr/src/app/.site_modules/node_modules/nuxt/bin/nuxt",
