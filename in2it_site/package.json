    "firebase": "7.9.3",
    "nuxt": "^2.10.2",
    "nuxt-env": "^0.1.0",
    "nuxt-google-maps-module": "^1.6.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "vue-badger-accordion": "^1.1.6",
    "vue-notification": "^1.3.20"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "jsdoc": "^3.6.3",
    "jsdoc-vuejs": "^3.0.5",
    "prettier": "^1.19.1",
